{
  "unocss.root": ["."],
  "unocss.colorPreview": true,
  "unocss.remToPxPreview": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.wordWrap": "on",
  "editor.tabSize": 2,
  "editor.snippetSuggestions": "top",
  "files.associations": {
    "*.js": "javascriptreact",
    "*.vue": "vue",
    "*.cshtml": "html",
    "*.dwt": "html"
  },
  "editor.formatOnPaste": true,
  "cSpell.words": [
    "Arrayable",
    "Attributify",
    "Axios",
    "cdnnode",
    "digitalacorn",
    "doption",
    "doptopns",
    "etags",
    "hoverable",
    "Logined",
    "openapi",
    "persistedstate",
    "Pinia",
    "postinstallmac",
    "Sider",
    "unocss",
    "unplugin",
    "VCLOUD",
    "workerorder"
  ],
  "unocss.underline": true,
  "vue.codeActions.enabled": false,
  "scss.validate": false,
  "less.validate": false,
  "css.validate": false,
  "css.lint.unknownAtRules": "ignore",
  "stylelint.enable": true,
  "stylelint.validate": ["html", "css", "less", "scss", "vue"],
  "typescript.tsdk": "node_modules/typescript/lib",
  "[xml]": {
    "editor.defaultFormatter": "DotJoshJohnson.xml"
  },
  "background.fullscreen": {
    "image": "",
    "images": ["file:///E:/images/ailiu.jpeg"],
    "opacity": 0.91,
    "size": "cover",
    "position": "center",
    "interval": 0
  },
  "background.style": {
    "content": "''",
    "pointer-events": "none",
    "position": "absolute",
    "z-index": "99999",
    "width": "100%",
    "height": "100%",
    "background-position": "100% 100%",
    "background-repeat": "no-repeat",
    "opacity": 1
  }
}
