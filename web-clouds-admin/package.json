{
  "name": "web-clouds-admin",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "pnpm gen-schema & pnpm run --filter \"./packages/*\" --parallel dev",
    "dev:prod": "pnpm gen-schema & pnpm run --filter \"./packages/*\" --parallel dev:prod",
    "dev:test": "pnpm gen-schema & pnpm run --filter \"./packages/*\" --parallel dev:test",
    "dev:base": "pnpm gen-schema & pnpm -C \"packages/pedestal\" run dev",
    "dev:dns": "pnpm gen-schema & pnpm -C \"packages/dns\" run dev",
    "build": "pnpm clean:out && pnpm gen-schema & pnpm run --filter \"./packages/*\" --parallel build",
    "build:release": "pnpm clean:out && pnpm gen-schema & pnpm run --filter \"./packages/*\" --parallel build:release",
    "build:pedestal": "pnpm gen-schema & pnpm -C \"packages/pedestal\" run build",
    "build:dns": "pnpm gen-schema & pnpm -C \"packages/dns\" run build",
    "build-dev": "pnpm clean:out && pnpm gen-schema & pnpm run --filter \"./packages/*\" --parallel build:dev",
    "build-dev:pedestal": "pnpm gen-schema & pnpm -C \"packages/pedestal\" run build:dev",
    "build-dev:dns": "pnpm gen-schema & pnpm -C \"packages/dns\" run build:dev",
    "build-test": "pnpm clean:out && pnpm gen-schema & pnpm run --filter \"./packages/*\" --parallel build:test",
    "build-test:pedestal": "pnpm gen-schema & pnpm -C \"packages/pedestal\" run build:test",
    "build-test:dns": "pnpm gen-schema & pnpm -C \"packages/dns\" run build:test",
    "gen-schema": "node ./packages/apis/schema-gen.mjs",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write packages/",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "postinstallmac": "git config core.hooksPath .husky && chmod 700 .husky/*",
    "eslint": "eslint --cache --max-warnings 0  \"packages/**/*.{vue,ts,js,tsx}\" --fix",
    "commit": "git add . && git-cz",
    "clean": "rimraf 'packages/*/{node_modules,dist}' && rimraf 'vca/*/{node_modules,dist}' && rimraf {node_modules,dist}",
    "clean:cache": "rimraf ./**/node_modules/.vite",
    "clean:out": "rimraf ./dist"
  },
  "dependencies": {
    "@antv/x6": "^2.11.3",
    "@antv/x6-vue-shape": "^2.0.11",
    "@plugin-web-update-notification/vite": "^1.6.4",
    "@types/uuid": "^9.0.2",
    "@vca/apis": "workspace:^",
    "@vca/shared": "workspace:^",
    "@vueuse/core": "^10.1.2",
    "dagre": "^0.8.5",
    "js-cookie": "^3.0.5",
    "pinia": "^2.0.36",
    "pinia-plugin-persistedstate": "^3.1.0",
    "tldts": "^6.0.11",
    "uuid": "^9.0.0",
    "vc-material": "^2.2.89",
    "visual-shield": "^1.0.0",
    "vue": "^3.3.4",
    "vue-accessible-color-picker": "^4.1.3",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.47.1",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-angular": "^17.6.3",
    "@commitlint/cz-commitlint": "^17.5.0",
    "@digitalacorn/vite-plugin-svg-icons": "3.0.0-pre.1",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.0",
    "@types/dagre": "^0.7.48",
    "@types/echarts": "^4.9.18",
    "@types/js-cookie": "^3.0.3",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.16.8",
    "@types/sortablejs": "^1.15.1",
    "@unocss/preset-rem-to-px": "^0.54.1",
    "@unocss/transformer-directives": "^0.53.1",
    "@unocss/transformer-variant-group": "^0.53.1",
    "@vitejs/plugin-legacy": "^4.0.3",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.3.2",
    "commitizen": "^4.3.0",
    "cz-customizable": "^6.9.2",
    "dayjs": "^1.11.8",
    "eslint": "^8.40.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^9.12.0",
    "husky": "^8.0.3",
    "inquirer": "^8.2.5",
    "less": "^4.1.3",
    "lint-staged": "^12.5.0",
    "lodash-es": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.25",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.8",
    "rollup-plugin-visualizer": "^5.9.0",
    "stylelint": "^15.10.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-less": "^1.0.4",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "~5.0.4",
    "unocss": "^0.53.1",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.4.9",
    "vite-plugin-html": "^3.2.0",
    "vue-tsc": "^1.8.8"
  },
  "lint-staged": {
    "*.{js,ts,vue,jsx,tsx}": [
      "npm run eslint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
